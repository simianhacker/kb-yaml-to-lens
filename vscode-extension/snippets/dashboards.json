{
  "Dashboard Structure": {
    "prefix": "dashboard",
    "body": [
      "---",
      "dashboards:",
      "  - name: '${1:Dashboard Name}'",
      "    description: ${2:Dashboard description}",
      "    ${3:controls:",
      "      - $4}",
      "    panels:",
      "      - $0"
    ],
    "description": "Create a new dashboard structure"
  },
  "Markdown Panel": {
    "prefix": "panel-markdown",
    "body": [
      "- title: ${1:Panel Title}",
      "  ${2:description: ${3:Panel description}}",
      "  ${4:hide_title: ${5|true,false|}}",
      "  size:",
      "    w: ${6|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${7:3}",
      "  ${8:position:",
      "    x: ${9:0}",
      "    y: ${10:0}}",
      "  markdown:",
      "    content: |",
      "      ${11:# Markdown content here",
      "      ",
      "      This panel supports **full Markdown syntax** including:",
      "      - Bullet lists",
      "      - [Links](https://example.com)",
      "      - Code blocks",
      "      ",
      "      See the [Markdown guide](https://www.markdownguide.org/) for more details.}"
    ],
    "description": "Add a Markdown panel with rich content"
  },
  "Search Panel": {
    "prefix": "panel-search",
    "body": [
      "- title: ${1:Recent Logs}",
      "  ${2:description: ${3:Shows the most recent log entries}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  search:",
      "    saved_search_id: ${9:your-saved-search-id}"
    ],
    "description": "Add a Search panel (requires a saved search ID from Kibana)"
  },
  "Links Panel": {
    "prefix": "panel-links",
    "body": [
      "- title: ${1:Quick Links}",
      "  ${2:description: ${3:Links to related dashboards and documentation}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:3}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  links:",
      "    layout: ${9|horizontal,vertical|}",
      "    items:",
      "      - label: ${10:Dashboard Link}",
      "        dashboard: ${11:dashboard-uuid-here}",
      "      - label: ${12:External Link}",
      "        url: ${13:https://example.com}",
      "      - label: ${14:Documentation}",
      "        url: ${15:https://docs.example.com}"
    ],
    "description": "Add a Links panel with multiple links to dashboards and URLs"
  },
  "Image Panel": {
    "prefix": "panel-image",
    "body": [
      "- title: ${1:Panel Title}",
      "  ${2:description: ${3:Panel description}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  image:",
      "    url: ${9:https://example.com/image.png}"
    ],
    "description": "Add an Image panel"
  },
  "Lens Metric Panel": {
    "prefix": "panel-lens-metric",
    "body": [
      "- title: ${1:Total Events}",
      "  ${2:description: ${3:Total count of events in the selected time range}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: metric",
      "    data_view: ${9:logs-*}",
      "    metric:",
      "      aggregation: ${10|count,sum,avg,min,max,median,unique_count|}",
      "      ${11:field: ${12:field_name}}"
    ],
    "description": "Add a Lens metric panel showing a single aggregated value"
  },
  "Lens Pie Chart": {
    "prefix": "panel-lens-pie",
    "body": [
      "- title: ${1:Top Values Distribution}",
      "  ${2:description: ${3:Shows the distribution of top values}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: pie",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: ${10:field_name}",
      "        type: values",
      "        size: ${11:5}",
      "    metric:",
      "      aggregation: ${12|count,sum,avg,min,max|}",
      "      ${13:field: ${14:numeric_field}}"
    ],
    "description": "Add a Lens pie chart showing top values distribution"
  },
  "Lens Line Chart": {
    "prefix": "panel-lens-line",
    "body": [
      "- title: ${1:Metric Over Time}",
      "  ${2:description: ${3:Shows how a metric changes over time}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: line",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: '@timestamp'",
      "        type: date_histogram",
      "    metrics:",
      "      - aggregation: ${10|count,sum,avg,min,max|}",
      "        ${11:field: ${12:numeric_field}}",
      "        ${13:label: ${14:Metric 1}}",
      "      ${15:- aggregation: ${16|count,sum,avg,min,max|}",
      "        field: ${17:another_field}",
      "        label: ${18:Metric 2}}"
    ],
    "description": "Add a Lens line chart showing metrics over time (supports multiple metrics)"
  },
  "Lens Bar Chart": {
    "prefix": "panel-lens-bar",
    "body": [
      "- title: ${1:Metric by Category}",
      "  ${2:description: ${3:Shows a metric broken down by category}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: bar",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: ${10:category_field}",
      "        type: ${11|values,date_histogram|}",
      "        ${12:size: ${13:10}}",
      "    metrics:",
      "      - aggregation: ${14|count,sum,avg,min,max|}",
      "        ${15:field: ${16:numeric_field}}",
      "        ${17:label: ${18:Metric Name}}"
    ],
    "description": "Add a Lens bar chart showing metrics by category"
  },
  "Lens Area Chart": {
    "prefix": "panel-lens-area",
    "body": [
      "- title: ${1:Cumulative Metric Over Time}",
      "  ${2:description: ${3:Shows how a metric accumulates over time}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: area",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: '@timestamp'",
      "        type: date_histogram",
      "    metrics:",
      "      - aggregation: ${10|count,sum,avg,min,max|}",
      "        ${11:field: ${12:numeric_field}}",
      "        ${13:label: ${14:Metric Name}}"
    ],
    "description": "Add a Lens area chart showing cumulative metrics over time"
  },
  "Lens Datatable": {
    "prefix": "panel-lens-datatable",
    "body": [
      "- title: ${1:Data Table}",
      "  ${2:description: ${3:Tabular view of data with multiple columns}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: datatable",
      "    data_view: ${9:logs-*}",
      "    columns:",
      "      - field: ${10:field_name}",
      "        type: ${11|values,date_histogram|}",
      "        ${12:label: ${13:Column 1}}",
      "      ${14:- field: ${15:another_field}",
      "        type: ${16|values,date_histogram|}",
      "        label: ${17:Column 2}}"
    ],
    "description": "Add a Lens datatable with multiple columns"
  },
  "Lens Gauge": {
    "prefix": "panel-lens-gauge",
    "body": [
      "- title: ${1:Metric Gauge}",
      "  ${2:description: ${3:Shows a metric as a gauge visualization}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: gauge",
      "    data_view: ${9:logs-*}",
      "    metric:",
      "      aggregation: ${10|count,sum,avg,min,max|}",
      "      ${11:field: ${12:numeric_field}}",
      "    ${13:minimum: ${14:0}}",
      "    ${15:maximum: ${16:100}}",
      "    ${17:goal: ${18:80}}",
      "    ${19:appearance:",
      "      shape: ${20|arc,circle,horizontalBullet,verticalBullet|}",
      "      color_mode: ${21|none,palette|}}"
    ],
    "description": "Add a Lens gauge showing a metric with optional min/max/goal values"
  },
  "Lens Mosaic Chart": {
    "prefix": "panel-lens-mosaic",
    "body": [
      "- title: ${1:Mosaic Chart}",
      "  ${2:description: ${3:Shows proportional data as nested rectangles}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: mosaic",
      "    data_view: ${9:logs-*}",
      "    dimension:",
      "      type: values",
      "      field: ${10:category_field}",
      "    ${11:breakdown:",
      "      type: values",
      "      field: ${12:breakdown_field}}",
      "    metric:",
      "      aggregation: ${13|count,sum,avg,min,max|}",
      "      ${14:field: ${15:numeric_field}}",
      "    ${16:titles_and_text:",
      "      value_format: ${17|percent,value,hidden|}",
      "      value_decimal_places: ${18:2}}",
      "    ${19:legend:",
      "      visible: ${20|show,hide,auto|}",
      "      position: ${21|right,bottom,left,top|}}"
    ],
    "description": "Add a Lens mosaic chart showing proportional categorical data"
  },
  "ESQL Metric Panel": {
    "prefix": "panel-esql-metric",
    "body": [
      "- title: ${1:ESQL Metric}",
      "  ${2:description: ${3:Metric calculated using ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: metric",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:total_count} = ${11:COUNT(*)}",
      "      ${12:| WHERE field == \"value\"}",
      "    primary:",
      "      field: ${10:total_count}"
    ],
    "description": "Add an ESQL metric panel using ES|QL query language"
  },
  "ESQL Line Chart": {
    "prefix": "panel-esql-line",
    "body": [
      "- title: ${1:ESQL Line Chart}",
      "  ${2:description: ${3:Time series data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: line",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:metric_value} = ${11:COUNT(*)} BY ${12:time_bucket} = BUCKET(@timestamp, 20, ?_tstart, ?_tend)",
      "      ${13:| WHERE field == \"value\"}",
      "    metrics:",
      "      - field: ${10:metric_value}"
    ],
    "description": "Add an ESQL line chart using ES|QL query language"
  },
  "ESQL Bar Chart": {
    "prefix": "panel-esql-bar",
    "body": [
      "- title: ${1:ESQL Bar Chart}",
      "  ${2:description: ${3:Categorical data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: bar",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:metric_value} = ${11:COUNT(*)} BY ${12:category_field}",
      "      ${13:| SORT ${10:metric_value} DESC}",
      "      ${14:| LIMIT 10}",
      "    metrics:",
      "      - field: ${10:metric_value}"
    ],
    "description": "Add an ESQL bar chart using ES|QL query language"
  },
  "ESQL Datatable": {
    "prefix": "panel-esql-datatable",
    "body": [
      "- title: ${1:ESQL Data Table}",
      "  ${2:description: ${3:Tabular data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: datatable",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      ${10:| WHERE field == \"value\"}",
      "      ${11:| KEEP @timestamp, message, host.name}",
      "      ${12:| SORT @timestamp DESC}",
      "      | LIMIT ${13:100}"
    ],
    "description": "Add an ESQL datatable using ES|QL query language"
  },
  "ESQL Area Chart": {
    "prefix": "panel-esql-area",
    "body": [
      "- title: ${1:ESQL Area Chart}",
      "  ${2:description: ${3:Cumulative time series data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: area",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:metric_value} = ${11:COUNT(*)} BY ${12:time_bucket} = BUCKET(@timestamp, 20, ?_tstart, ?_tend)",
      "      ${13:| SORT ${12:time_bucket} ASC}",
      "    metrics:",
      "      - field: ${10:metric_value}"
    ],
    "description": "Add an ESQL area chart showing cumulative metrics over time"
  },
  "ESQL Gauge": {
    "prefix": "panel-esql-gauge",
    "body": [
      "- title: ${1:ESQL Gauge}",
      "  ${2:description: ${3:Gauge visualization from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: gauge",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:metric_value} = ${11|AVG(field),COUNT(*),SUM(field),MAX(field),MIN(field)|}",
      "    metric:",
      "      field: ${10:metric_value}",
      "    ${12:minimum: ${13:0}}",
      "    ${14:maximum: ${15:100}}",
      "    ${16:goal: ${17:80}}",
      "    ${18:appearance:",
      "      shape: ${19|arc,circle,horizontalBullet,verticalBullet|}",
      "      color_mode: ${20|none,palette|}}"
    ],
    "description": "Add an ESQL gauge using ES|QL query language"
  },
  "ESQL Pie Chart": {
    "prefix": "panel-esql-pie",
    "body": [
      "- title: ${1:ESQL Pie Chart}",
      "  ${2:description: ${3:Pie chart from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: pie",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:slice_count} = COUNT(*) BY ${11:category_field}",
      "      ${12:| SORT ${10:slice_count} DESC}",
      "      ${13:| LIMIT 10}",
      "    metrics:",
      "      - field: ${10:slice_count}",
      "    dimensions:",
      "      - field: ${11:category_field}",
      "    ${14:appearance:",
      "      donut: ${15|small,medium,large|}}",
      "    ${16:legend:",
      "      visible: ${17|show,hide,auto|}",
      "      position: ${18|right,bottom,left,top|}}"
    ],
    "description": "Add an ESQL pie chart using ES|QL query language"
  },
  "ESQL Mosaic Chart": {
    "prefix": "panel-esql-mosaic",
    "body": [
      "- title: ${1:ESQL Mosaic Chart}",
      "  ${2:description: ${3:Mosaic chart from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: mosaic",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:value_count} = COUNT(*) BY ${11:category_field}${12:, ${13:breakdown_field}}",
      "    metric:",
      "      field: ${10:value_count}",
      "    dimension:",
      "      field: ${11:category_field}",
      "    ${14:breakdown:",
      "      field: ${13:breakdown_field}}",
      "    ${15:titles_and_text:",
      "      value_format: ${16|percent,value,hidden|}",
      "      value_decimal_places: ${17:2}}",
      "    ${18:legend:",
      "      visible: ${19|show,hide,auto|}",
      "      position: ${20|right,bottom,left,top|}}"
    ],
    "description": "Add an ESQL mosaic chart using ES|QL query language"
  },
  "Control - Options List": {
    "prefix": "control-options",
    "body": [
      "- type: options",
      "  ${1:title: ${2:Filter by Category}}",
      "  data_view: ${3:logs-*}",
      "  field: ${4:category_field}",
      "  ${5:width: ${6|small,medium,large|}}"
    ],
    "description": "Add an options list control for filtering by field values"
  },
  "Control - Range Slider": {
    "prefix": "control-range",
    "body": [
      "- type: range",
      "  ${1:title: ${2:Numeric Range}}",
      "  data_view: ${3:logs-*}",
      "  field: ${4:numeric_field}",
      "  ${5:width: ${6|small,medium,large|}}"
    ],
    "description": "Add a range slider control for filtering numeric values"
  },
  "Control - Time Slider": {
    "prefix": "control-time",
    "body": [
      "- type: time",
      "  ${1:title: ${2:Time Range}}",
      "  ${3:start_offset: ${4:0.0}}",
      "  ${5:end_offset: ${6:1.0}}"
    ],
    "description": "Add a time slider control for filtering by time range"
  },
  "Control - ESQL Static Values": {
    "prefix": "control-esql-static",
    "body": [
      "- type: esql",
      "  variable_name: ${1:variable_name}",
      "  variable_type: ${2|values,multi_values,fields,time_literal,functions|}",
      "  choices:",
      "    - ${3:option1}",
      "    - ${4:option2}",
      "    - ${5:option3}",
      "  label: ${6:Filter Label}",
      "  ${7:width: ${8|small,medium,large|}}",
      "  ${9:multiple: ${10|true,false|}}"
    ],
    "description": "Add an ESQL static values control with predefined options for filtering ESQL visualizations"
  },
  "Control - ESQL Query": {
    "prefix": "control-esql-query",
    "body": [
      "- type: esql",
      "  variable_name: ${1:variable_name}",
      "  variable_type: ${2|values,multi_values,fields,time_literal,functions|}",
      "  query: ${3:FROM logs-* | STATS count = COUNT(*) BY ${4:field_name} | KEEP ${4:field_name} | LIMIT 20}",
      "  label: ${5:Filter Label}",
      "  ${6:width: ${7|small,medium,large|}}",
      "  ${8:multiple: ${9|true,false|}}"
    ],
    "description": "Add an ESQL query control that dynamically fetches filter values from an ESQL query"
  },
  "Lens Tagcloud Panel": {
    "prefix": "panel-lens-tagcloud",
    "body": [
      "- title: ${1:Tag Cloud}",
      "  ${2:description: ${3:Word cloud showing term frequency}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: tagcloud",
      "    data_view: ${9:logs-*}",
      "    dimension:",
      "      type: values",
      "      field: ${10:field_name}",
      "    metric:",
      "      aggregation: ${11|count,sum,avg,min,max|}",
      "      ${12:field: ${13:numeric_field}}",
      "    ${14:appearance:",
      "      min_font_size: ${15:12}",
      "      max_font_size: ${16:72}",
      "      orientation: ${17|single,right angled,multiple|}",
      "      show_label: ${18|true,false|}}"
    ],
    "description": "Add a Lens tag cloud panel showing term frequency as a word cloud"
  },
  "ESQL Tagcloud Panel": {
    "prefix": "panel-esql-tagcloud",
    "body": [
      "- title: ${1:ESQL Tag Cloud}",
      "  ${2:description: ${3:Word cloud from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: tagcloud",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:tag_count} = COUNT(*) BY ${11:tag_field}",
      "      ${12:| SORT ${10:tag_count} DESC}",
      "      ${13:| LIMIT 100}",
      "    dimension:",
      "      field: ${11:tag_field}",
      "    metric:",
      "      field: ${10:tag_count}",
      "    ${14:appearance:",
      "      min_font_size: ${15:12}",
      "      max_font_size: ${16:72}",
      "      orientation: ${17|single,right angled,multiple|}",
      "      show_label: ${18|true,false|}}"
    ],
    "description": "Add an ESQL tag cloud panel using ES|QL query language"
  },
  "Lens Heatmap Panel": {
    "prefix": "panel-lens-heatmap",
    "body": [
      "- title: ${1:Heatmap}",
      "  ${2:description: ${3:Heatmap showing data distribution}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
