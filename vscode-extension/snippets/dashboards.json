{
  "Dashboard Structure": {
    "prefix": "dashboard",
    "body": [
      "---",
      "dashboards:",
      "  - name: '${1:Dashboard Name}'",
      "    description: ${2:Dashboard description}",
      "    ${3:controls:",
      "      - $4}",
      "    panels:",
      "      - $0"
    ],
    "description": "Create a new dashboard structure"
  },
  "Markdown Panel": {
    "prefix": "panel-markdown",
    "body": [
      "- title: ${1:Panel Title}",
      "  ${2:description: ${3:Panel description}}",
      "  ${4:hide_title: ${5|true,false|}}",
      "  size:",
      "    w: ${6|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${7:3}",
      "  ${8:position:",
      "    x: ${9:0}",
      "    y: ${10:0}}",
      "  markdown:",
      "    content: |",
      "      ${11:# Markdown content here",
      "      ",
      "      This panel supports **full Markdown syntax** including:",
      "      - Bullet lists",
      "      - [Links](https://example.com)",
      "      - Code blocks",
      "      ",
      "      See the [Markdown guide](https://www.markdownguide.org/) for more details.}"
    ],
    "description": "Add a Markdown panel with rich content"
  },
  "Search Panel": {
    "prefix": "panel-search",
    "body": [
      "- title: ${1:Recent Logs}",
      "  ${2:description: ${3:Shows the most recent log entries}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  search:",
      "    saved_search_id: ${9:your-saved-search-id}"
    ],
    "description": "Add a Search panel (requires a saved search ID from Kibana)"
  },
  "Links Panel": {
    "prefix": "panel-links",
    "body": [
      "- title: ${1:Quick Links}",
      "  ${2:description: ${3:Links to related dashboards and documentation}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:3}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  links:",
      "    layout: ${9|horizontal,vertical|}",
      "    links:",
      "      - label: ${10:Dashboard Link}",
      "        dashboard: ${11:dashboard-uuid-here}",
      "      - label: ${12:External Link}",
      "        url: ${13:https://example.com}",
      "      - label: ${14:Documentation}",
      "        url: ${15:https://docs.example.com}"
    ],
    "description": "Add a Links panel with multiple links to dashboards and URLs"
  },
  "Image Panel": {
    "prefix": "panel-image",
    "body": [
      "- title: ${1:Panel Title}",
      "  ${2:description: ${3:Panel description}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  image:",
      "    url: ${9:https://example.com/image.png}"
    ],
    "description": "Add an Image panel"
  },
  "Lens Metric Panel": {
    "prefix": "panel-lens-metric",
    "body": [
      "- title: ${1:Total Events}",
      "  ${2:description: ${3:Total count of events in the selected time range}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: metric",
      "    data_view: ${9:logs-*}",
      "    metric:",
      "      aggregation: ${10|count,sum,avg,min,max,median,unique_count|}",
      "      ${11:field: ${12:field_name}}"
    ],
    "description": "Add a Lens metric panel showing a single aggregated value"
  },
  "Lens Pie Chart": {
    "prefix": "panel-lens-pie",
    "body": [
      "- title: ${1:Top Values Distribution}",
      "  ${2:description: ${3:Shows the distribution of top values}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: pie",
      "    data_view: ${9:logs-*}",
      "    slice_by:",
      "      - field: ${10:field_name}",
      "        type: values",
      "        size: ${11:5}",
      "    metric:",
      "      aggregation: ${12|count,sum,avg,min,max|}",
      "      ${13:field: ${14:numeric_field}}"
    ],
    "description": "Add a Lens pie chart showing top values distribution"
  },
  "Lens Line Chart": {
    "prefix": "panel-lens-line",
    "body": [
      "- title: ${1:Metric Over Time}",
      "  ${2:description: ${3:Shows how a metric changes over time}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: line",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: '@timestamp'",
      "        type: date_histogram",
      "    metrics:",
      "      - aggregation: ${10|count,sum,avg,min,max|}",
      "        ${11:field: ${12:numeric_field}}",
      "        ${13:label: ${14:Metric 1}}",
      "      ${15:- aggregation: ${16|count,sum,avg,min,max|}",
      "        field: ${17:another_field}",
      "        label: ${18:Metric 2}}"
    ],
    "description": "Add a Lens line chart showing metrics over time (supports multiple metrics)"
  },
  "Lens Bar Chart": {
    "prefix": "panel-lens-bar",
    "body": [
      "- title: ${1:Metric by Category}",
      "  ${2:description: ${3:Shows a metric broken down by category}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: bar",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: ${10:category_field}",
      "        type: ${11|values,date_histogram|}",
      "        ${12:size: ${13:10}}",
      "    metrics:",
      "      - aggregation: ${14|count,sum,avg,min,max|}",
      "        ${15:field: ${16:numeric_field}}",
      "        ${17:label: ${18:Metric Name}}"
    ],
    "description": "Add a Lens bar chart showing metrics by category"
  },
  "Lens Area Chart": {
    "prefix": "panel-lens-area",
    "body": [
      "- title: ${1:Cumulative Metric Over Time}",
      "  ${2:description: ${3:Shows how a metric accumulates over time}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: area",
      "    data_view: ${9:logs-*}",
      "    dimensions:",
      "      - field: '@timestamp'",
      "        type: date_histogram",
      "    metrics:",
      "      - aggregation: ${10|count,sum,avg,min,max|}",
      "        ${11:field: ${12:numeric_field}}",
      "        ${13:label: ${14:Metric Name}}"
    ],
    "description": "Add a Lens area chart showing cumulative metrics over time"
  },
  "Lens Datatable": {
    "prefix": "panel-lens-datatable",
    "body": [
      "- title: ${1:Data Table}",
      "  ${2:description: ${3:Tabular view of data with multiple columns}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: datatable",
      "    data_view: ${9:logs-*}",
      "    columns:",
      "      - field: ${10:field_name}",
      "        type: ${11|values,date_histogram|}",
      "        ${12:label: ${13:Column 1}}",
      "      ${14:- field: ${15:another_field}",
      "        type: ${16|values,date_histogram|}",
      "        label: ${17:Column 2}}"
    ],
    "description": "Add a Lens datatable with multiple columns"
  },
  "Lens Gauge": {
    "prefix": "panel-lens-gauge",
    "body": [
      "- title: ${1:Metric Gauge}",
      "  ${2:description: ${3:Shows a metric as a gauge visualization}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: gauge",
      "    data_view: ${9:logs-*}",
      "    metric:",
      "      aggregation: ${10|count,sum,avg,min,max|}",
      "      ${11:field: ${12:numeric_field}}",
      "    ${13:max: ${14:100}}"
    ],
    "description": "Add a Lens gauge showing a metric with optional max value"
  },
  "ESQL Metric Panel": {
    "prefix": "panel-esql-metric",
    "body": [
      "- title: ${1:ESQL Metric}",
      "  ${2:description: ${3:Metric calculated using ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:8}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: metric",
      "    query:",
      "      esql: |",
      "        FROM ${9:logs-*}",
      "        | ${10:STATS count()}",
      "        ${11:| WHERE field == \"value\"}"
    ],
    "description": "Add an ESQL metric panel using ES|QL query language"
  },
  "ESQL Line Chart": {
    "prefix": "panel-esql-line",
    "body": [
      "- title: ${1:ESQL Line Chart}",
      "  ${2:description: ${3:Time series data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: line",
      "    query:",
      "      esql: |",
      "        FROM ${9:logs-*}",
      "        | ${10:STATS count() BY @timestamp}",
      "        ${11:| WHERE field == \"value\"}"
    ],
    "description": "Add an ESQL line chart using ES|QL query language"
  },
  "ESQL Bar Chart": {
    "prefix": "panel-esql-bar",
    "body": [
      "- title: ${1:ESQL Bar Chart}",
      "  ${2:description: ${3:Categorical data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: bar",
      "    query:",
      "      esql: |",
      "        FROM ${9:logs-*}",
      "        | ${10:STATS count() BY category_field}",
      "        ${11:| SORT count() DESC}",
      "        ${12:| LIMIT 10}"
    ],
    "description": "Add an ESQL bar chart using ES|QL query language"
  },
  "ESQL Datatable": {
    "prefix": "panel-esql-datatable",
    "body": [
      "- title: ${1:ESQL Data Table}",
      "  ${2:description: ${3:Tabular data from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: datatable",
      "    query:",
      "      esql: |",
      "        FROM ${9:logs-*}",
      "        ${10:| WHERE field == \"value\"}",
      "        ${11:| KEEP @timestamp, message, host.name}",
      "        ${12:| SORT @timestamp DESC}",
      "        | LIMIT ${13:100}"
    ],
    "description": "Add an ESQL datatable using ES|QL query language"
  },
  "Control - Options List": {
    "prefix": "control-options",
    "body": [
      "- type: options",
      "  ${1:title: ${2:Filter by Category}}",
      "  data_view: ${3:logs-*}",
      "  field: ${4:category_field}",
      "  ${5:width: ${6|small,medium,large|}}"
    ],
    "description": "Add an options list control for filtering by field values"
  },
  "Control - Range Slider": {
    "prefix": "control-range",
    "body": [
      "- type: range",
      "  ${1:title: ${2:Numeric Range}}",
      "  data_view: ${3:logs-*}",
      "  field: ${4:numeric_field}",
      "  ${5:width: ${6|small,medium,large|}}"
    ],
    "description": "Add a range slider control for filtering numeric values"
  },
  "Control - Time Slider": {
    "prefix": "control-time",
    "body": [
      "- type: time",
      "  ${1:title: ${2:Time Range}}",
      "  ${3:start_offset: ${4:0.0}}",
      "  ${5:end_offset: ${6:1.0}}"
    ],
    "description": "Add a time slider control for filtering by time range"
  },
  "Control - ESQL Static Values": {
    "prefix": "control-esql-static",
    "body": [
      "- type: esql",
      "  variable_name: ${1:variable_name}",
      "  variable_type: ${2|values,multi_values,fields,time_literal,functions|}",
      "  choices:",
      "    - ${3:option1}",
      "    - ${4:option2}",
      "    - ${5:option3}",
      "  label: ${6:Filter Label}",
      "  ${7:width: ${8|small,medium,large|}}",
      "  ${9:multiple: ${10|true,false|}}"
    ],
    "description": "Add an ESQL static values control with predefined options for filtering ESQL visualizations"
  },
  "Control - ESQL Query": {
    "prefix": "control-esql-query",
    "body": [
      "- type: esql",
      "  variable_name: ${1:variable_name}",
      "  variable_type: ${2|values,multi_values,fields,time_literal,functions|}",
      "  query: ${3:FROM logs-* | STATS count = COUNT(*) BY ${4:field_name} | KEEP ${4:field_name} | LIMIT 20}",
      "  label: ${5:Filter Label}",
      "  ${6:width: ${7|small,medium,large|}}",
      "  ${8:multiple: ${9|true,false|}}"
    ],
    "description": "Add an ESQL query control that dynamically fetches filter values from an ESQL query"
  },
  "Lens Tagcloud Panel": {
    "prefix": "panel-lens-tagcloud",
    "body": [
      "- title: ${1:Tag Cloud}",
      "  ${2:description: ${3:Word cloud showing term frequency}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: tagcloud",
      "    data_view: ${9:logs-*}",
      "    tags:",
      "      type: values",
      "      field: ${10:field_name}",
      "    metrics:",
      "      aggregation: ${11|count,sum,avg,min,max|}",
      "      ${12:field: ${13:numeric_field}}",
      "    ${14:appearance:",
      "      min_font_size: ${15:12}",
      "      max_font_size: ${16:72}",
      "      orientation: ${17|single,right angled,multiple|}",
      "      show_label: ${18|true,false|}}"
    ],
    "description": "Add a Lens tag cloud panel showing term frequency as a word cloud"
  },
  "ESQL Tagcloud Panel": {
    "prefix": "panel-esql-tagcloud",
    "body": [
      "- title: ${1:ESQL Tag Cloud}",
      "  ${2:description: ${3:Word cloud from ESQL query}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:12}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: tagcloud",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:tag_count} = COUNT(*) BY ${11:tag_field}",
      "      ${12:| SORT ${10:tag_count} DESC}",
      "      ${13:| LIMIT 100}",
      "    tags:",
      "      field: ${11:tag_field}",
      "    metrics:",
      "      field: ${10:tag_count}",
      "    ${14:appearance:",
      "      min_font_size: ${15:12}",
      "      max_font_size: ${16:72}",
      "      orientation: ${17|single,right angled,multiple|}",
      "      show_label: ${18|true,false|}}"
    ],
    "description": "Add an ESQL tag cloud panel using ES|QL query language"
  },
  "ESQL Heatmap Panel": {
    "prefix": "panel-esql-heatmap",
    "body": [
      "- title: ${1:ESQL Heatmap}",
      "  ${2:description: ${3:Heatmap showing data distribution}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: heatmap",
      "    query: |",
      "      FROM ${9:logs-*}",
      "      | STATS ${10:event_count} = COUNT(*) BY ${11:x_field}, ${12:y_field}",
      "      ${13:| LIMIT 100}",
      "    x_axis:",
      "      field: ${11:x_field}",
      "    ${14:y_axis:",
      "      field: ${12:y_field}}",
      "    value:",
      "      field: ${10:event_count}",
      "    ${15:legend:",
      "      is_visible: ${16|true,false|}",
      "      position: ${17|right,bottom,left,top|}}",
      "    ${18:grid_config:",
      "      is_cell_label_visible: ${19|false,true|}",
      "      is_x_axis_label_visible: ${20|true,false|}",
      "      is_y_axis_label_visible: ${21|true,false|}}"
    ],
    "description": "Add an ESQL heatmap panel showing data distribution across two dimensions"
  },
  "ESQL XY Chart with Appearance": {
    "prefix": "panel-esql-xy-advanced",
    "body": [
      "- title: ${1:ESQL Chart with Appearance}",
      "  ${2:description: ${3:Advanced ESQL chart with appearance options}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  esql:",
      "    type: ${9|line,bar,area|}",
      "    query: |",
      "      FROM ${10:logs-*}",
      "      | STATS ${11:metric_value} = ${12|COUNT(*),AVG(field),SUM(field)|} BY ${13:time_bucket} = BUCKET(@timestamp, 1 hour)",
      "      ${14:| ORDER ${13:time_bucket} ASC}",
      "    dimensions:",
      "      - field: ${13:time_bucket}",
      "    metrics:",
      "      - field: ${11:metric_value}",
      "        ${15:id: ${16:metric_1}}",
      "    ${17:appearance:",
      "      y_left_axis:",
      "        title: ${18:Y-Axis Title}",
      "        scale: ${19|linear,log,sqrt|}",
      "      ${20:series:",
      "        - metric_id: ${16:metric_1}",
      "          color: ${21:#2196F3}}}",
      "    ${22:legend:",
      "      visible: ${23|true,false|}",
      "      position: ${24|right,bottom,left,top|}}"
    ],
    "description": "Add an advanced ESQL XY chart with appearance and legend options"
  },
  "XY Chart with Reference Line": {
    "prefix": "panel-xy-refline",
    "body": [
      "- title: ${1:Metric with Threshold}",
      "  ${2:description: ${3:Chart with reference line threshold}}",
      "  size:",
      "    w: ${4|whole,half,third,quarter,sixth,eighth,12,24,48|}",
      "    h: ${5:16}",
      "  ${6:position:",
      "    x: ${7:0}",
      "    y: ${8:0}}",
      "  lens:",
      "    type: ${9|line,bar,area|}",
      "    data_view: ${10:logs-*}",
      "    dimensions:",
      "      - type: date_histogram",
      "        field: '@timestamp'",
      "    metrics:",
      "      - aggregation: ${11|count,avg,sum,min,max|}",
      "        ${12:field: ${13:numeric_field}}",
      "        ${14:id: ${15:main_metric}}",
      "    layers:",
      "      - type: reference_line",
      "        data_view: ${10:logs-*}",
      "        reference_lines:",
      "          - label: ${16:Threshold}",
      "            value: ${17:100.0}",
      "            color: ${18:#FF0000}",
      "            line_style: ${19|solid,dashed,dotted|}",
      "            ${20:line_width: ${21:2}}",
      "            ${22:fill: ${23|none,above,below|}}"
    ],
    "description": "Add an XY chart with reference line layer for thresholds or SLA markers"
  }
}
